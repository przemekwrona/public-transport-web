{
  "name": "public-transport-web-client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration development --ssr=false",
    "build:prod": "ng build --configuration production --ssr=false --output-path ../public-transport-web-server/src/main/resources/static",
    "generate": "npm run i18n:extract & npm run generate:api & npm run generate:pdf:api & npm run generate:planner:api",
    "generate:api": "openapi-generator-cli generate -i ../public-transport-web-server/src/main/resources/api/public-transport-api.yaml -g typescript-angular -o ./src/app/generated/public-transport-api",
    "generate:pdf:api": "openapi-generator-cli generate -i ../public-transport-web-server/src/main/resources/api/public-transport-pdf-api.yaml -g typescript-angular -o ./src/app/generated/public-transport-pdf-api",
    "generate:planner:api": "openapi-generator-cli generate -i ../public-transport-web-server/src/main/resources/api/public-transport-planner-api.yaml -g typescript-angular -o ./src/app/generated/public-transport-planner-api",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "mock:server": "mockserver -p 3000 -m mock",
    "i18n:extract": "npx transloco-keys-manager extract --input src --output src/assets/i18n --langs pl en",
    "i18n:find": "npx transloco-keys-manager find"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.18",
    "@angular/cdk": "^19.2.19",
    "@angular/common": "^19.2.18",
    "@angular/compiler": "^19.2.18",
    "@angular/core": "^19.2.18",
    "@angular/forms": "^19.2.18",
    "@angular/material": "^19.2.19",
    "@angular/material-luxon-adapter": "^19.2.19",
    "@angular/platform-browser": "^19.2.18",
    "@angular/platform-browser-dynamic": "^19.2.18",
    "@angular/router": "^19.2.18",
    "@fortawesome/angular-fontawesome": "1.0.0",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@googlemaps/polyline-codec": "^1.0.28",
    "@jsverse/transloco": "^7.6.1",
    "@popperjs/core": "2.11.8",
    "@types/leaflet": "^1.9.8",
    "@types/lodash": "^4.17.1",
    "font-awesome": "^4.7.0",
    "google-polyline": "^1.0.3",
    "haversine-distance": "^1.2.4",
    "leaflet": "^1.9.4",
    "lodash": "^4.17.21",
    "mini-svg-data-uri": "^1.4.4",
    "moment": "^2.30.1",
    "ngx-drag-drop": "^19.0.0",
    "ngx-gauge": "^11.0.0",
    "ngx-image-cropper": "^9.1.5",
    "ngx-mask": "^20.0.3",
    "ngx-mat-select-search": "^8.0.4",
    "ngx-material-timepicker": "^13.1.1",
    "ngx-sortable": "^2.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.19",
    "@angular/cli": "^19.2.19",
    "@angular/compiler-cli": "^19.2.18",
    "@jsverse/transloco-keys-manager": "^6.2.2",
    "@openapitools/openapi-generator-cli": "^2.20.2",
    "@types/jasmine": "~5.1.0",
    "autoprefixer": "^10.4.20",
    "gbfs-typescript-types": "^1.0.7",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "mockserver": "^3.1.1",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3"
  }
}
